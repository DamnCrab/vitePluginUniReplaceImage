{
    "name": "vite-plugin-uni-replace-image",
    "version": "0.0.6",
    "description": "A Vite plugin to replace UniApp image tracking points with Base64 data",
    "repository": {
        "type": "git",
        "url": "https://github.com/DamnCrab/vitePluginUniReplaceImage"
    },
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite build --watch",
        "build": "vite build",
        "test": "vitest",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "vite",
        "uniapp",
        "image",
        "replace"
    ],
    "author": "DamnCrab",
    "license": "MIT",
    "peerDependencies": {
        "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.11.0",
        "typescript": "^5.3.0",
        "vite": "^7.0.0",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^4.0.15"
    }
}