{
    "name": "vite-plugin-uni-replace-image",
    "version": "0.0.1",
    "description": "A Vite plugin to replace images in uniapp builds",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite build --watch",
        "build": "vite build",
        "test": "vitest",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "vite",
        "uniapp",
        "image",
        "replace"
    ],
    "author": "DamnCrab",
    "license": "MIT",
    "peerDependencies": {
        "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.11.0",
        "typescript": "^5.3.0",
        "vite": "^5.0.0",
        "vitest": "^2.1.8"
    }
}